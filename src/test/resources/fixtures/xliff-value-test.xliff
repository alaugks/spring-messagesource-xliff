       xsi:schemaLocation="urn:oasis:names:tc:xliff:document:2.1 xliff-core-2.1.xsd">
    <file datatype="plaintext" srcLang="en" trgLang="de">
        <unit id="1">
            <segment id="element">
                <source>value</source>
                <target>value</target>
            </segment>
